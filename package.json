{
  "name": "tn-text",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc && DEBUG=tn-text:* npm start",
    "docker-up": "tsc && docker-compose up -d",
    "docker-down": "docker-compose down",
    "docker-downv": "docker-compose down -v",
    "docker-res": "npm run docker-down && npm run docker-up",
    "migrate": "node-pg-migrate up",
    "rollback": "node-pg-migrate down",
    "start": "node --env-file=.env ./dist/main.js"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.19.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg-promise": "^11.9.0",
    "pug": "^3.0.3",
    "twilio": "^5.2.2",
    "typescript": "^5.5.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.6",
    "@types/pug": "^2.0.10",
    "@types/ws": "^8.5.11",
    "node-pg-migrate": "^7.5.2"
  }
}
